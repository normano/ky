{
  "compilerOptions": {
    "declaration": true,
    "moduleResolution": "Node16",
    "sourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "stripInternal": true,
    "importsNotUsedAsValues": "remove"
  }
}